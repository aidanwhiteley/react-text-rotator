{
  "name": "react-text-rotator",
  "version": "1.0.0",
  "description": "Simple React component for rotating text",
  "main": "lib/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js --env prod",
    "start": "webpack serve --config ./demo/webpack.config.js --mode=development",
    "demo:build": "rm -rf ./demo/dist && webpack --config ./demo/webpack.config.js",
    "demo:deploy": "npm run demo:build && gh-pages -d demo/dist",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test": "jest"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-react": "^7.21.5",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "style-loader": "^2.0.0",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "author": "Clayton Marinho <claytonmarinho@gmail.com>",
  "homepage": "https://claytonmarinho.github.io/react-text-rotator/",
  "license": "MIT",
  "repository": "https://github.com/claytonmarinho/react-text-rotator",
  "keywords": [
    "react",
    "component",
    "text",
    "rotator"
  ]
}
